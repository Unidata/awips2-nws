<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
   <bean id="replacestationsrequest" class="gov.noaa.nws.ocp.edex.climate.service.handler.configparams.ReplaceStationsServiceHandler"/>
  
    <bean factory-bean="handlerRegistry" factory-method="register">
      <constructor-arg value="gov.noaa.nws.ocp.common.dataplugin.climate.request.configparams.ReplaceStationsServiceRequest"/>
      <constructor-arg ref="replacestationsrequest"/>
    </bean>

   <bean id="snowperciprequest" class="gov.noaa.nws.ocp.edex.climate.service.handler.configparams.SnowPrecipServiceHandler"/>
  
    <bean factory-bean="handlerRegistry" factory-method="register">
      <constructor-arg value="gov.noaa.nws.ocp.common.dataplugin.climate.request.configparams.SnowPrecipServiceRequest"/>
      <constructor-arg ref="snowperciprequest"/>
    </bean>
</beans>