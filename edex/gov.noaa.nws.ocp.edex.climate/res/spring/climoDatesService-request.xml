<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="climodatesservice"
        class="gov.noaa.nws.ocp.edex.climate.service.handler.ClimoDatesServiceHandler" />

    <bean factory-bean="handlerRegistry" factory-method="register">
        <constructor-arg
            value="gov.noaa.nws.ocp.common.dataplugin.climate.request.ClimoDatesRequest" />
        <constructor-arg ref="climodatesservice" />
    </bean>

</beans>