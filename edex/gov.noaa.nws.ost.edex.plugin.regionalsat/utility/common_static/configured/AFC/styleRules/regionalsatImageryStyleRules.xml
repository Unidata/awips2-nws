<styleRuleset>

<!-- Regional Satellite Rules -->
<styleRule>
  <paramLevelMatches>
     <parameter>Imager 11 micron IR</parameter>
      <creatingEntity>GOESR-PG</creatingEntity>
  </paramLevelMatches>
  <imageStyle>
     <range scale="LINEAR">
        <minValue>128</minValue>
        <maxValue>-127</maxValue>
     </range>
     <defaultColormap>Sat/IR/CIRA (IR Default)</defaultColormap> 
  </imageStyle>
</styleRule>
<styleRule>
  <paramLevelMatches>
     <parameter>Imager Based Derived Precipitable Water (PW)</parameter>
      <creatingEntity>Blended2</creatingEntity>
  </paramLevelMatches>
  <imageStyle>
     <range scale="LINEAR">
        <minValue>1</minValue>
        <maxValue>254</maxValue>
     </range>
     <defaultColormap>Sat/Precip Water/Precip Water Default</defaultColormap> 
  </imageStyle>
</styleRule>
</styleRuleset>