<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!--
        This is an absolute override file, indicating that a higher priority
        version of the file will completely replace a lower priority version
        of the file. Currently only BASE and SITE are read by AWIPS.
-->
<ClimateIngestConfig xmlns:ns2="group">
    <!-- Example of the site level climate_ingest_config.xm for OAX
    <filter>
        <sourceIngest group="STATION">
            <dataKey>KOMA</dataKey>
            <dataKey>KOAX</dataKey>
            <dataKey>KLNK</dataKey>
            <dataKey>KOFK</dataKey>
            <dataKey>KFNB</dataKey>
            <dataKey>KTOE</dataKey>
        </sourceIngest>
    </filter> 
    -->
    <filter>
        <!-- Allowed ICAO (station-code) list, or ALL for any station. Default NONE -->
        <sourceIngest group="STATION">
            <dataKey>NONE</dataKey>
        </sourceIngest>
    </filter>
</ClimateIngestConfig>
