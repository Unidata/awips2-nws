<menuTemplate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <contribute xsi:type="bundleItem" file="bundles/griddednucaps/griddednucaps.xml"
        menuText="Temperature 2m" id="griddednucapsproduct">
        <substitute key="param" value="T2m" />
        <substitute key="level" value="FHAG" />
        <substitute key="levelOne" value="2.0" />
        <substitute key="colormap" value="Grid/gridded data" />
    </contribute>
    <contribute xsi:type="bundleItem" file="bundles/griddednucaps/griddednucaps.xml"
        menuText="Relative Humidity 2m" id="griddednucapsproduct">
        <substitute key="param" value="RH" />
        <substitute key="level" value="FHAG" />
        <substitute key="levelOne" value="2.0" />
        <substitute key="colormap" value="Grid/gridded data" />
    </contribute>
    <!-- TODO Critical temperature Heights -->
    
    <contribute xsi:type="bundleItem" file="bundles/griddednucaps/griddednucaps.xml"
        menuText="Ozone Anomaly" id="griddednucapsproduct">
        <substitute key="param" value="OZA" />
        <substitute key="level" value="EA" />
        <substitute key="levelOne" value="0.0" />
        <substitute key="colormap" value="Grid/OzoneAnomaly" />
    </contribute>
    <contribute xsi:type="bundleItem" file="bundles/griddednucaps/griddednucaps.xml"
        menuText="Total Ozone" id="griddednucapsproduct">
        <substitute key="param" value="TOZNE" />
        <substitute key="level" value="EA" />
        <substitute key="levelOne" value="0.0" />
        <substitute key="colormap" value="Grid/TotalOzone" />
    </contribute>
    <contribute xsi:type="bundleItem" file="bundles/griddednucaps/griddednucaps.xml"
        menuText="Tropopause Level" id="griddednucapsproduct">
        <substitute key="param" value="TPH" />
        <substitute key="level" value="EA" />
        <substitute key="levelOne" value="0.0" />
        <substitute key="colormap" value="Grid/TropopauseLevel" />
    </contribute>
    <contribute xsi:type="subMenu" menuText="Haines Index" id="HI">
        <contribute xsi:type="subinclude" fileName="menus/griddednucaps/HainesIndexProducts.xml">
            <substitute key="param" value="HI"/>
            <substitute key="colorvar" value="Grid/gridded data"/>
        </contribute>
    </contribute>
    <!-- TODO Mixing Height -->
    <contribute xsi:type="bundleItem" file="bundles/griddednucaps/griddednucaps.xml"
        menuText="Quality Control" id="griddednucapsproduct">
        <substitute key="param" value="QUAL" />
        <substitute key="level" value="EA" />
        <substitute key="levelOne" value="0.0" />
        <substitute key="interpolate" value="false" />
        <substitute key="colormap" value="Grid/griddedNUCAPS_QC" />
    </contribute>
